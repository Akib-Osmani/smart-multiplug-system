// Pin assignments for the relays
const int relay1Pin = 26;  // Relay 1 connected to GPIO 12
const int relay2Pin = 43;  // Relay 2 connected to GPIO 13

void setup() {
  // Initialize the relay pins as outputs
  pinMode(relay1Pin, OUTPUT);
  pinMode(relay2Pin, OUTPUT);

  // Start with both relays turned off
  digitalWrite(relay1Pin, LOW);
  digitalWrite(relay2Pin, LOW);
}

void loop() {
  // Turn Relay 1 ON and Relay 2 OFF
  digitalWrite(relay1Pin, HIGH);
  digitalWrite(relay2Pin, LOW);
  delay(2000);  // Wait for 2 seconds

  // Turn Relay 1 OFF and Relay 2 ON
  digitalWrite(relay1Pin, LOW);
  digitalWrite(relay2Pin, HIGH);
  delay(2000);  // Wait for 2 seconds
}
